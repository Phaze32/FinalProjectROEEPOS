                        <asp:MenuItem NavigateUrl="~/Marketing.aspx" Text="Marketing"/>
						<asp:MenuItem NavigateUrl="~/Products.aspx" Text="Products"/>
                        <asp:MenuItem NavigateUrl="~/stock.aspx" Text="Stock"/>
                        <asp:MenuItem NavigateUrl="~/FinancialReports.aspx" Text="Finance"/>
                        <asp:MenuItem NavigateUrl="~/HR.aspx" Text="HR"/>
                        <asp:MenuItem NavigateUrl="~/admin.aspx" Text="ADMIN"/>
